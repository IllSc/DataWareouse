          SQLTask:Connection="{451D0438-60D8-4BF2-AD08-E6F38EB24ED4}"
          SQLTask:SqlStatementSource="CREATE TABLE [dbo].[dimClient]&#xA;(&#xA;&#x9;[CID] INT IDENTITY(1,1) NOT NULL,&#xA;&#x9;&#x9;[client_id] int,&#xA;&#xA;&#x9;[client] varchar(100),&#xA;&#xA;&#x9;[category] varchar(10),&#xA;&#x9;&#xA;&#x9;[current] bit,&#xA;&#x9;&#xA;&#x9;primary key clustered(CID)&#xA;&#xA;);&#xA;&#xA;&#xA;&#xA;CREATE TABLE [dimCity](&#xA;&#x9;&#xA;&#x9;[city_id] int,&#xA;&#x9;&#xA;&#x9;[city] varchar(100),&#xA;&#x9;&#xA;&#x9;[state_id] int,&#xA;&#x9;&#xA;&#x9;[state] varchar(100),&#xA;&#xA;&#x9;[country_id] int,&#xA;&#x9;&#xA;&#x9;[country_name] varchar(100),&#xA;&#x9;&#xA;&#x9;primary key clustered(city_id)  &#xA;&#xA;);&#xA;&#xA;&#xA;&#xA;CREATE TABLE [dimDate] (&#xA;&#x9;&#xA;&#x9;[DateID] int IDENTITY(1,1) NOT NULL,&#xA;&#x9;&#xA;&#x9;[Date] date,&#xA;&#x9;&#xA;&#x9;[day] int,&#xA;&#xA;&#x9;[month] int,&#xA;&#x9;&#xA;&#x9;[year] int,&#xA;&#x9;&#xA;&#x9;primary key clustered(DateID)&#xA;&#xA;);&#xA;&#xA;&#xA;&#xA;CREATE TABLE dimEmployee(&#xA;&#x9;&#xA;&#x9;[EID] INT IDENTITY(1,1) NOT NULL,&#xA;&#x9;&#x9;[emp_no] int,&#xA;&#x9;&#xA;&#x9;[birth_date] int,&#xA;&#x9;&#xA;&#x9;[first_name] varchar(100),&#xA;&#x9;&#xA;&#x9;[last_name] varchar(100),&#xA;&#x9;&#xA;&#x9;[gender] varchar(10),&#xA;&#x9;&#xA;&#x9;[marital_status] varchar(10),&#xA;&#x9;&#xA;&#x9;[no_of_children] int,&#xA;&#x9;&#xA;&#x9;[hire_date] int,&#xA;&#x9;&#xA;&#x9;[title_id] int,&#xA;&#x9;&#xA;&#x9;[title] varchar(50),&#xA;&#x9;&#xA;&#x9;[current_city_id] int,&#xA;&#x9;&#xA;&#x9;[base_city_id] int,&#xA;&#x9;&#xA;&#x9;[dept_id] char(4),&#xA;&#xA;&#x9;[dept_name] varchar(40),&#xA;&#xA;&#x9;[start] date,&#xA;&#xA;&#x9;[end] date,&#xA;&#xA;&#x9;primary key clustered(EID),&#xA;&#xA;&#x9;constraint fk_employee_basecity foreign key(base_city_id) references dimCity (city_id),&#xA;&#x9;&#x9;constraint fk_employee_currentcity foreign key (current_city_id) references dimCity (city_id),&#xA;&#xA;&#x9;constraint fk_employee_birthdate foreign key (birth_date) references dimDate (DateID),&#xA;&#xA;&#x9;constraint fk_employee_hiredate foreign key (hire_date) references dimDate (DateID),&#xA;&#xA;);&#xA;&#xA;&#xA;&#xA;CREATE TABLE [bridgeEmployeeClient] (&#xA;&#x9;&#xA;&#x9;[BID] INT IDENTITY(1,1) NOT NULL,&#xA;&#xA;&#x9;[CID] int,&#xA;&#xA;&#x9;[EID] int,&#xA;&#xA;&#x9;[start] date,&#xA;&#xA;&#x9;[end] date,&#xA;&#xA;&#x9;primary key(BID),&#xA;&#xA;&#x9;constraint fk_bridge_client foreign key (CID) references dimClient (CID),&#xA;&#xA;&#x9;constraint fk_bridge_employee foreign key (EID) references dimEmployee (EID)&#xA;&#xA;);&#xA;&#xA;&#xA;&#xA;CREATE TABLE [factSalary] (&#xA;&#x9;&#xA;&#x9;[EID] int,&#xA;&#xA;&#x9;[DateID] int,&#xA;&#xA;&#x9;[salary] int,&#xA;&#xA;&#x9;primary key (EID, DateID),&#xA;&#xA;&#x9;constraint fk_salary_date foreign key (DateID) references dimDate (DateID),&#xA;&#xA;&#x9;constraint fk_salary_employee foreign key (EID) references dimEmployee (EID)&#xA;&#xA;&#xA;);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
